{
  "name": "query-input",
  "version": "0.0.0",
  "scripts": {
    "start": "webpack-dev-server --mode=development --host=0.0.0.0 --devtool=eval-source-map",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "lint": "eslint src/**/*.js"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "quotes": [
        "error",
        "single"
      ],
      "object-curly-spacing": [
        "error",
        "never"
      ]
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "transform": {
      ".*": "babel-jest"
    }
  },
  "devDependencies": {
    "@babel/preset-env": "7.8.4",
    "babel-jest": "25.1.0",
    "css-loader": "3.4.2",
    "eslint": "6.8.0",
    "jest": "25.1.0",
    "jest-cli": "25.1.0",
    "style-loader": "1.1.3",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.3"
  }
}
